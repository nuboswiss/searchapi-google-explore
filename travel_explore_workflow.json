    {
      "parameters": {
        "jsCode": "// Extract destinations from the API response\nconst destinations = items[0].json.destinations || [];\n\n// Sort destinations by flight price (cheapest first)\nconst sortedDestinations = destinations.sort((a, b) => {\n  const priceA = a.flight?.price ?? Infinity;\n  const priceB = b.flight?.price ?? Infinity;\n  return priceA - priceB;\n});\n\n// Return sorted destinations as individual items\nreturn sortedDestinations.map(dest => ({ json: dest }));"
      },
      "id": "sort-by-price",
      "name": "Sort by Cheapest Price",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [900, 300]
    },
    {
      "parameters": {
        "maxItems": "={{ $('Set Default Parameters').item.json.limit || 20 }}"
      },
      "id": "limit-results",
      "name": "Limit Results",
      "type": "n8n-nodes-base.limit",
      "typeVersion": 1,
      "position": [1120, 300]
    },
    {
      "parameters": {
        "jsCode": "// Get the currency from the parameters\nconst currency = $('Set Default Parameters').first().json.currency || 'CHF';\n\n// Format each destination with rank\nreturn items.map((item, index) => {\n  const dest = item.json;\n  const rank = index + 1;\n  \n  const name = dest.name || 'Unknown';\n  const country = dest.country || 'Unknown';\n  const flight = dest.flight || {};\n  \n  const price = flight.price ?? 'N/A';\n  const airline = flight.airline_name || 'Unknown';\n  const stops = flight.stops;\n  const duration = flight.flight_duration || 'N/A';\n  \n  const outbound = dest.outbound_date || 'N/A';\n  const returnDate = dest.return_date || 'N/A';\n  \n  const avgCostNight = dest.avg_cost_per_night;\n  const costStr = avgCostNight ? `${avgCostNight} ${currency}/night` : 'N/A';\n  \n  const stopText = stops === 0 ? 'direct' : `${stops} stop(s)`;\n  \n  return {\n    json: {\n      rank,\n      destination: `${name}, ${country}`,\n      flight_price: price,\n      flight_price_formatted: `${price} ${currency}`,\n      airline,\n      stops: stopText,\n      duration,\n      outbound_date: outbound,\n      return_date: returnDate,\n      avg_accommodation_cost: costStr,\n      // Keep original data for reference\n      raw: dest\n    }\n  };\n});"
      },
      "id": "format-output",
      "name": "Format Output",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
